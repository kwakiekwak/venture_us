<div id="show-friends">

<!-- search form for friends -->
<form method="GET" action="/ventures/new">
  <input id="email-search" type="text" name="email" placeholder="Search by Email">
  <button type="submit">Search</button>
</form>
  <% users.forEach(function (user){ %>
  <li><%= user.local.email %><button class="add-friend-btn" value="<%=user.id%>">+</button></li>
  <% }) %>
    </ul>
</div>
