<!-- page where you see your venture - pull in Foursquare/YELP-->
<!DOCTYPE html>
<html>
<head>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="/css/new_venture.scss">

</head>
<body>
<%- include('../partials/header', {subHeading: 'Ventures Below'}) %>
<main>

<div class="container">
  <div class='row'>
    <div class='col-md-12 section'>
  <br>
  <div id="myCarousel" class="carousel slide" data-ride="carousel">
  <div id="outerCarousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
      <br><br>
      <div class="item active">
        <img src="http://www.comohotels.com/metropolitanbangkok/sites/default/files/styles/background_image/public/images/background/metbkk_bkg_nahm_restaurant.jpg?itok=GSmnYYaU" alt="Chania" width="460" height="345">
        <div class="carousel-caption">Yum!</div>
      </div>
      <div class="item">
        <img src="https://cdn3.vox-cdn.com/thumbor/BUIzK8lQauIzEocPiqqPjTGWFFk=/899x600/cdn0.vox-cdn.com/uploads/chorus_asset/file/814400/03_2014_SONG-6_JPG.0.jpg" alt="Chania" width="460" height="345">
      </div>
      <div class="item">
        <img src="http://keepinitkind.com/wp-content/uploads/2013/10/SAGE-16.jpg" alt="Flower" width="460" height="345">
      </div>
    </div>
    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
    </div>

  </div>
  <!-- NOTE: set a variable for location (search params above) to be the query
  for this aPI call. -->
  <iframe src="https://www.google.com/maps/embed/v1/place?q=LosAngeles,CA%20Rd,%20UK
     &zoom=17
     &key=AIzaSyCfWNPOPK8qDWvOmPr0q8ldYgRkDU6qGIk">
 </iframe>

  <%- include('../partials/footer') %>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>

 </main>
</body>
</html>
