<!-- Temporary view page for listing top 20 results
of a categorical search -->
<h1>Venues matching your search criteria <%=query %> for <%=location%>: </h1>

<!-- Here, loop through the venues -->
<!-- NOTE: for loop up to i = 20, incorporate carousel -->
<% var max20 = [] %>
<% venues.venues.forEach(function(venue) { %>
  <!-- Now, loop through that array 20 times and render. -->
  <% max20.push(venue) %>
<% }) %>
<!-- Edit the slice, based on # of users (40 divided by number of users) -->
  <% var limit = max20.slice(0,20) %>
  <% limit.forEach(function(venue) { %>
    <ul>
      <li><strong><%= venue.name %></strong> -located at <%= venue.location.address %></li>
      <li><%=venue.id%></li>
    </ul>
  <% }) %>

<iframe src='https://www.google.com/maps/embed/v1/place?q=<%=location%>&zoom=17&key=AIzaSyBVa8h6g2RoCSSDZi7NyC7UPUn7Z-YDjrM'>
 </iframe>



