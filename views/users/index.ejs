<!DOCTYPE html>
<html>
<head>
  <%- include('../partials/head') %>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>  <style>
      * { margin: 0; padding: 0; box-sizing: border-box; }
      body { font: 13px Helvetica, Arial; }
      form { background: #000; padding: 3px; position: fixed; bottom: 0; width: 100%; }
      form input { border: 0; padding: 10px; width: 90%; margin-right: .5%; }
      form button { width: 9%; background: rgb(130, 224, 255); border: none; padding: 10px; }
      #messages { list-style-type: none; margin: 0; padding: 0; }
      #messages li { padding: 5px 10px; }
      #messages li:nth-child(odd) { background: #eee; }
    </style>
</head>
<body>
  <%- include('../partials/header', {subHeading: 'User List'}) %>

  <main>
    <ul>
      <% users.forEach(function(user) { %>
        <li><a href="/users/<%= user._id %>"><%= user.name %></a></li>
      <% }) %>
    </ul>
    <p><a href="/">Back to home.</a></p>

  </main>

     <ul id="messages">Messages below!</ul>
    <form action="">
      <input id="m" autocomplete="off" /><button>Send</button>
    </form>
  <script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io();
</script>
  <%- include('../partials/scripts') %>
</body>
</html>
